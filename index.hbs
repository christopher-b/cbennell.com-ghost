{{!< default}}

<article-list>
  <h1>Articles</h1>

  {{#get "posts" filter="tag:-lab" as |posts|}}
  {{#foreach posts}}
  {{> "components/card" page=../pagination.page }}
  {{/foreach}}
  {{/get}}
</article-list>

{{!-- If JS isn't enabled, fallback to standard pagination --}}
<noscript>
  {{ pagination }}
</noscript>